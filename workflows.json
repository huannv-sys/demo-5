{
  "streamlit": {
    "command": "streamlit run app.py --server.port=5000",
    "reloadConditions": [
      "always",
      "fileChanged"
    ],
    "onlyReloadIfExited": false,
    "waitExplicitReload": false,
    "requireExitCodeZero": false
  },
  "node": {
    "command": "node server.js",
    "reloadConditions": [
      "always",
      "fileChanged"
    ],
    "onlyReloadIfExited": false,
    "waitExplicitReload": false,
    "requireExitCodeZero": false
  }
}